#include <stdio.h>

float somaf (float num1, float num2){
    float resultado;
    resultado = num1 + num2;
    return resultado;
}

float subtracaof (float num1, float num2){
    float resultado;
    resultado = num1 - num2;
    return resultado;
}

float multiplicação (float num1, float num2){
    float resultado;
    resultado = num1 * num2;
    return resultado;
}

float divisao (float num1, float num2){
    float resultado;
    resultado = num1 / num2;
    return resultado;
}

float potencia (float num1, float num2){
    float resultado;
    for(int i = 0; i < num2; i++){
        resultado *= num1;
    }
    return resultado;
}

float porcentagem (float num1, float num2){
    float resultado;
    resultado = num1 / 100 * num2;
    return resultado;
}

int main() {
    
    printf("Bem vindo a calculadora do SYMINHO!!!\n\n\n");
    
    
    printf("\nEscolha um operador lógico?");
    printf("\n[+] - Soma");
    printf("\n[-] - Subtração");
    printf("\n[*] - Multiplicação");
    printf("\n[/] - Divisão");
    printf("\n[^] - Potencia");
    printf("\n[%] - Porcentagem");

    
    return 0;
}
